{
  "name": "donallynch/youtube-api",
  "description": "YOUTUBE VIDEO API SEARCH DEMO",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.2",
    "ext-dom": "20031129",
    "ext-gd": "^7.2",
    "ext-json": "1.6.0",
    "ext-pcre": "^7.2",
    "ext-redis": "4.1.1",
    "ext-tokenizer": "^7.2",
    "ext-xml": "^7.2",
    "fideloper/proxy": "4.2.*",
    "google/apiclient": "^2.0",
    "guzzlehttp/guzzle": "^6.4.1",
    "laravel/framework": "^6.4.0",
    "laravel/helpers": "^1.1",
    "laravel/tinker": "^1.0.10",
    "laravelcollective/html": "^6.0.3"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.1",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
